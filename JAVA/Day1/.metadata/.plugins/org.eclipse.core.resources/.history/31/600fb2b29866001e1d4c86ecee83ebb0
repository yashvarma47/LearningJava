package com.training.org;

class Student{
	String name;

	void printInfo() {
		
		System.out.println(this.name);
		System.out.println(this.age);
	}
}



public class Javaoops {

	public static void main(String[] args) {
		Student s1 = new Student();
		int age;
		s1.name = "Yash";
		s1.age = 23;
		s1.printInfo();

	}

}
